   "source": [
    "# 1) Group for ALL transactions\n",
    "emt_all = emt.groupby(['customer_id', 'transaction_date']).agg(\n",
    "    total_amount=('amount_cad','sum'),\n",
    "    total_count=('amount_cad','count')\n",
    ")\n",
    "\n",
    "# 2) Group for DEBIT only\n",
    "emt_debit = (\n",
    "    emt[emt['debit_credit']=='D']\n",
    "    .groupby(['customer_id','transaction_date'])\n",
    "    .agg(\n",
    "        total_amount_debit=('amount_cad','sum'),\n",
    "        total_count_debit=('amount_cad','count')\n",
    "    )\n",
    ")\n",
    "\n",
    "# 3) Group for CREDIT only (optional)\n",
    "emt_credit = (\n",
    "    emt[emt['debit_credit']=='C']\n",
    "    .groupby(['customer_id','transaction_date'])\n",
    "    .agg(\n",
    "        total_amount_credit=('amount_cad','sum'),\n",
    "        total_count_credit=('amount_cad','count')\n",
    "    )\n",
    ")\n",
    "\n",
    "# Join them together\n",
    "emt_summary = (\n",
    "    emt_all\n",
    "    .join(emt_debit, how='left')\n",
    "    .join(emt_credit, how='left')\n",
    "    .fillna(0)\n",
    "    .reset_index()\n",
    ")"
   ]
  },
